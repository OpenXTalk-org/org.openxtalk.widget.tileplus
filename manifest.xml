<package version="0.0">
  <name>org.openxtalk.widget.tileplus</name>
  <title>Tile Plus</title>
  <author>OpemXTalk.org, LiveCode Ltd</author>
  <version>1.0.0</version>
  <license>community</license>
  <type>widget</type>
  <requires name="com.livecode.canvas"/>
  <requires name="com.livecode.library.widgetutils"/>
  <requires name="com.livecode.library.iconsvg"/>
  <requires name="com.livecode.widget"/>
  <requires name="com.livecode.array"/>
  <requires name="com.livecode.list"/>
  <requires name="com.livecode.arithmetic"/>
  <requires name="com.livecode.binary"/>
  <requires name="com.livecode.logic"/>
  <requires name="com.livecode.string"/>
  <requires name="com.livecode.type"/>
  <requires name="com.livecode.math"/>
  <requires name="com.livecode.engine"/>
  <metadata key="preferredSize">200,200</metadata>
  <metadata key="userVisible">true</metadata>
  <metadata key="sourceColor.editor">com.livecode.pi.color</metadata>
  <metadata key="sourceColor.default">blue</metadata>
  <metadata key="tileLabel.editor">com.livecode.pi.string</metadata>
  <metadata key="tileLabel.default">Label</metadata>
  <metadata key="colorStep.editor">com.livecode.pi.string</metadata>
  <metadata key="colorStep.default">0</metadata>
  <property name="tileLabel" get="string" set="string"/>
  <property name="iconName" get="string" set="string"/>
  <property name="hoverIconName" get="string" set="string"/>
  <property name="hoverState" get="boolean" set="boolean"/>
  <property name="fgColor" get="Color" set="string"/>
  <property name="fgHoverColor" get="Color" set="string"/>
  <property name="bgColor" get="Color" set="string"/>
  <property name="bgHoverColor" get="Color" set="string"/>
  <property name="innerShadowAngle" get="number" set="number"/>
  <metadata key="innerShadowAngle.editor">com.livecode.pi.number</metadata>
  <metadata key="innerShadowAngle.label">Inner Shadow angle</metadata>
  <metadata key="innerShadowAngle.step">1</metadata>
  <metadata key="innerShadowAngle.default">105</metadata>
  <metadata key="innerShadowAngle.min">0</metadata>
  <metadata key="innerShadowAngle.max">359</metadata>
  <metadata key="innerShadowAngle.section">Effects</metadata>
  <property name="innerShadowDistance" get="number" set="number"/>
  <metadata key="innerShadowDistance.editor">com.livecode.pi.number</metadata>
  <metadata key="innerShadowDistance.label">Inner Shadow distance</metadata>
  <metadata key="innerShadowDistance.step">1</metadata>
  <metadata key="innerShadowDistance.default">7</metadata>
  <metadata key="innerShadowDistance.min">0</metadata>
  <metadata key="innerShadowDistance.max">255</metadata>
  <metadata key="innerShadowDistance.section">Effects</metadata>
  <property name="innerShadowColor" get="string" set="string"/>
  <metadata key="innerShadowColor.editor">com.livecode.pi.color</metadata>
  <metadata key="innerShadowColor.label">Inner Shadow color</metadata>
  <metadata key="innerShadowColor.section">Effects</metadata>
  <property name="innerShadowSize" get="number" set="number"/>
  <metadata key="innerShadowSize.editor">com.livecode.pi.number</metadata>
  <metadata key="innerShadowSize.section">Effects</metadata>
  <metadata key="innerShadowSize.label">Inner Shadow size</metadata>
  <metadata key="innerShadowSize.step">1</metadata>
  <metadata key="innerShadowSize.default">15</metadata>
  <metadata key="innerShadowSize.min">0</metadata>
  <metadata key="innerShadowSize.max">255</metadata>
  <metadata key="innerShadowSize.section">Effects</metadata>
  <metadata key="textFont.user_visible">false</metadata>
  <metadata key="textSize.user_visible">false</metadata>
  <property name="labelTextFont" get="string" set="string"/>
  <metadata key="labelTextFont.editor">com.livecode.pi.font</metadata>
  <metadata key="labelTextFont.default">Helvetica</metadata>
  <metadata key="labelTextFont.section">Text</metadata>
  <metadata key="labelTextFont.label">Label Text Font</metadata>
  <property name="labelTextSize" get="number" set="number"/>
  <metadata key="labelTextSize.editor">com.livecode.pi.number</metadata>
  <metadata key="labelTextSize.default">12</metadata>
  <metadata key="labelTextSize.section">Text</metadata>
  <metadata key="labelTextSize.label">Label Text Size</metadata>
  <handler name="OnSave" parameters="out array" return="any"/>
  <handler name="OnLoad" parameters="in array" return="any"/>
  <handler name="onCreate" parameters="" return="any"/>
  <handler name="OnPaint" parameters="" return="any"/>
  <handler name="setTileLabel" parameters="in string" return="nothing"/>
  <handler name="OnMouseUp" parameters="" return="any"/>
  <handler name="OnMouseEnter" parameters="" return="any"/>
  <handler name="OnMouseLeave" parameters="" return="any"/>
  <handler name="setIconName" parameters="in string" return="nothing"/>
  <handler name="setHoverIconName" parameters="in string" return="nothing"/>
  <handler name="setForegroundColor" parameters="in string" return="nothing"/>
  <handler name="setForegroundHoverColor" parameters="in string" return="nothing"/>
  <handler name="setBackgroundColor" parameters="in string" return="nothing"/>
  <handler name="setBackgroundHoverColor" parameters="in string" return="nothing"/>
  <handler name="setHover" parameters="in boolean" return="nothing"/>
</package>